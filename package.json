{
  "name": "datarag",
  "version": "0.8.0",
  "description": "Have real time discussions with your data",
  "main": "src/index.js",
  "scripts": {
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "migrate_undo": "./node_modules/.bin/sequelize db:migrate:undo",
    "dev": "nodemon",
    "start": "node src/index.js",
    "eslint": "eslint src/ tests/",
    "test:setup": "psql postgresql://postgres:postgres@datarag-postgres -c 'create database test;' && psql postgresql://postgres:postgres@datarag-postgres -c 'grant all privileges on database test to postgres;'",
    "test:teardown": "psql postgresql://postgres:postgres@datarag-postgres -c 'drop database if exists test;'",
    "test": "npm run test:teardown && npm run test:setup && NODE_ENV=test npm run migrate && NODE_ENV=test NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --maxWorkers=1 --forceExit"
  },
  "author": "Datarag",
  "license": "Apache-2.0",
  "engines": {
    "node": "20.x.x"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./tests/jest.setup.js"
    ]
  },
  "dependencies": {
    "@opendocsg/pdf2md": "^0.2.1",
    "@sentry/node": "^8.47.0",
    "axios": "^1.7.9",
    "body-parser": "^1.20.2",
    "bull": "^4.16.5",
    "cheerio": "^1.0.0",
    "cohere-ai": "^7.15.0",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "express": "^4.21.2",
    "express-openapi-validator": "^5.3.9",
    "ioredis": "^5.4.2",
    "js-tiktoken": "^1.0.16",
    "lodash": "^4.17.21",
    "marked": "^15.0.4",
    "nanoid": "^3.3.8",
    "nconf": "^0.12.1",
    "newrelic": "^12.9.0",
    "nunjucks": "^3.2.4",
    "openai": "^4.77.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "pgvector": "^0.2.0",
    "private-ip": "^2.3.4",
    "redlock": "^5.0.0-beta.2",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "swagger-jsdoc": "^6.2.8",
    "turndown": "^7.2.0",
    "wink-eng-lite-web-model": "^1.8.1",
    "wink-nlp": "^2.3.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0"
  }
}
